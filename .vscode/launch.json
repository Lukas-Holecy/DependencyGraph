                ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console":  "internalConsole"
        },
        {
            "name": "OfficeUtilitiesOnly",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
